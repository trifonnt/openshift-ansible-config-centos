[OSEv3:children]
masters
nodes

[OSEv3:vars]
ansible_ssh_user=root
deployment_type=origin
openshift_master_identity_providers=[{'name': 'htpasswd_auth', 'login': 'true', 'challenge': 'true', 'kind': 'HTPasswdPasswordIdentityProvider', 'filename': '/etc/origin/master/htpasswd'}]
containerized=true
openshift_release=v1.4
openshift_image_tag=v1.4.0
openshift_public_hostname=console.localdomain
openshift_master_default_subdomain=apps.console.localdomain
openshift_hosted_metrics_deploy=false


[masters]
console.localdomain openshift_schedulable=false

# host group for etcd
[etcd]
console.localdomain

[nodes]
console.localdomain openshift_node_labels="{'region': 'infra', 'zone': 'default'}" openshift_schedulable=false
node01.localdomain openshift_node_labels="{'region': 'eu', 'zone': 'south'}" openshift_schedulable=true
node02.localdomain openshift_node_labels="{'region': 'eu', 'zone': 'east'}" openshift_schedulable=true
