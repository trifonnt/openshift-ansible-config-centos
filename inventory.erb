[OSEv3:children]
masters
nodes

[OSEv3:vars]
ansible_ssh_user=root
deployment_type=origin
openshift_master_identity_providers=[{'name': 'htpasswd_auth', 'login': 'true', 'challenge': 'true', 'kind': 'HTPasswdPasswordIdentityProvider', 'filename': '/etc/origin/master/htpasswd'}]
containerized=false
openshift_release=v1.5.1
openshift_image_tag=v1.5.1
openshift_public_hostname=console.trifonnt.com
openshift_master_default_subdomain=apps.trifonnt.com
openshift_hosted_metrics_deploy=false

#openshift_master_overwrite_named_certificates=true
#openshift_master_named_certificates=[{"certfile": "/etc/letsencrypt/live/console.techdope.io/cert.pem", "keyfile": "/etc/letsencrypt/live/console.techdope.io/privkey.pem", "cafile": "/etc/letsencrypt/live/console.techdope.io/fullchain.pem"}]


[masters]
console.trifonnt.com openshift_schedulable=true
#console.trifonnt.com openshift_schedulable=false

# host group for etcd
#[etcd]
#console.trifonnt.com

[nodes]
console.trifonnt.com openshift_node_labels="{'region': 'infra', 'zone': 'default'}" openshift_schedulable=true
#node01.trifonnt.com openshift_node_labels="{'region': 'eu', 'zone': 'south'}" openshift_schedulable=true
#node02.trifonnt.com openshift_node_labels="{'region': 'eu', 'zone': 'east'}" openshift_schedulable=true
